{
  "name": "pretty-imports",
  "displayName": "pretty_imports",
  "description": "extension which takes imports and organises them into pep8 style with clean visual style",
  "version": "0.0.2",
  "engines": {
    "vscode": "^1.82.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
          "command": "extension.prettyImports",
          "title": "Pretty Imports"
      }
  ],
  "menus": {
    "editor/context": [
        {
            "command": "extension.prettyImports",
            "when": "editorHasSelection"
        }
    ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.82.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^6.4.1",
    "@typescript-eslint/parser": "^6.4.1",
    "eslint": "^8.47.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.1.6",
    "@vscode/test-electron": "^2.3.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ChanrajMandok/pretty_imports_vsc_extension.git"
  },
"publisher": "ChanrajMandok"
}